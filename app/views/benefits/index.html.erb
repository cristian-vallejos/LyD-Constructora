<p id="notice"><%= notice %></p>

<h1>Benefits</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Area</th>
      <th>Costotrabajador</th>
      <th>Costoempresa</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @benefits.each do |benefit| %>
      <tr>
        <td><%= benefit.nombre %></td>
        <td><%= benefit.area %></td>
        <td><%= benefit.costotrabajador %></td>
        <td><%= benefit.costoempresa %></td>
        <td><%= link_to 'Show', benefit %></td>
        <td><%= link_to 'Edit', edit_benefit_path(benefit) %></td>
        <td><%= link_to 'Destroy', benefit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Benefit', new_benefit_path %>
