<table>
  <thead>
    <tr>
      <th>id</th>
      <th>Rut</th>
      <th>Nombre</th>
      <th>Cencos</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  <%if params[:search] && params[:search]!= "" %>
      <% @results.each do |employee|%>
        <tr>
            <td><%= employee.id %></td>
            <td><%= employee.rut %></td>
            <td><%= employee.nombre %></td>
            <td><%= employee.cencos %></td>
            <td><%= employee.estado %></td>
            <td><%= link_to 'Agregar Familia', new_familiartrabajador_path(:rut_trabajador => employee.rut ) %></td>
        </tr>
        <%end%>
  <%end %>
</tbody>

</table>